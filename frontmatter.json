{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "previewPath": null,
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "Description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "Publishing date",
          "name": "date",
          "type": "datetime",
          "default": "{{now}}",
          "isPublishDate": true
        },
        {
          "title": "Content preview",
          "name": "preview",
          "type": "image"
        },
        {
          "title": "Is in draft",
          "name": "draft",
          "type": "draft"
        },
        {
          "title": "Tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "Categories",
          "name": "categories",
          "type": "categories"
        },
        {
          "title": "Navigation weight",
          "name": "weight",
          "type": "number"
        }
      ]
    }
  ],
  "frontMatter.framework.id": "hugo",
  "frontMatter.content.publicFolder": {
    "path": "assets/images",
    "relative": true
  },
  "frontMatter.preview.host": "http://localhost:1313",
  "frontMatter.content.pageFolders": [
    {
      "path": "[[workspace]]/.history",
      "excludeSubdir": true,
      "title": ".history"
    },
    {
      "path": "[[workspace]]/themes",
      "excludeSubdir": true,
      "title": "themes"
    },
    {
      "title": "ai",
      "path": "[[workspace]]/content/ai",
      "contentTypes": [
        "documentation"
      ]
    },
    {
      "title": "backend",
      "path": "[[workspace]]/content/backend",
      "contentTypes": [
        "documentation"
      ]
    },
    {
      "title": "data",
      "path": "[[workspace]]/content/data",
      "contentTypes": [
        "documentation"
      ]
    },
    {
      "title": "dev-ops",
      "path": "[[workspace]]/content/dev-ops",
      "contentTypes": [
        "documentation"
      ]
    },
    {
      "title": "frontend",
      "path": "[[workspace]]/content/frontend",
      "contentTypes": [
        "documentation"
      ]
    },
    {
      "title": "notes",
      "path": "[[workspace]]/content/notes",
      "contentTypes": [
        "documentation"
      ]
    },
    {
      "title": "programming",
      "path": "[[workspace]]/content/programming",
      "contentTypes": [
        "documentation"
      ]
    },
    {
      "title": "gallery",
      "path": "[[workspace]]/content/gallery",
      "contentTypes": [
        "blog"
      ]
    },
    {
      "title": "diary",
      "path": "[[workspace]]/content/diary",
      "contentTypes": [
        "blog"
      ]
    },
    {
      "path": "[[workspace]]/assets/images",
      "title": "images"
    },
    {
      "title": "kubernetes",
      "path": "[[workspace]]/content/dev-ops/container/container-orchestration/kubernetes"
    }
  ],
  "frontMatter.framework.startCommand": "npm run dev"
}